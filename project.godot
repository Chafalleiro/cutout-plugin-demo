; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="cutout plugin demo"
run/main_scene="res://player/player.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/image="res://addons/bodypartcontrol/icons/vga_64.png"
config/icon="res://addons/bodypartcontrol/icons/vga_64.png"

[editor]

version_control/plugin_name="Godot Git Plugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/bodypartcontrol/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
